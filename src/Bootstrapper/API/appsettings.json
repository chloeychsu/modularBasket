{
  "ConnectionStrings": {
    "Database": "Server=localhost;Port=5432;Database=basketdb;User Id=postgres;Password=postgres;Include Error Detail=true",
    "Redis": "localhost:6379"
  },
  "MessageBroker":{
    "Host":"amqp://localhost:5672",
    "UserName":"guest",
    "Password":"guest"
  },
  "Keycloak": {
    "realm": "myrealm",
    "auth-server-url": "http://localhost:9090/",
    "ssl-required": "none",
    "resource": "myclient",
    "verify-token-audience": false,
    "credentials": {
    "secret": ""
    },
    "confidential-port": 0
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console","Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl":"http://localhost:5341"
        }
      }
    ],
    "Enrich": ["FromLogContext","WithMachineName","WithProcessId","WithThreadId"],
    "Properties": {
      "Application": "ModularBasket ASP.NET Core App",
      "Evironment": "Development"
    }
  }, 
  "AllowedHosts": "*"
}